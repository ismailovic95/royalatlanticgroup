{
    "name": "wpdesk\/wp-canada-post-shipping-method",
    "description": "Canada Post Shipping Method",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "woocommerce",
        "shipping",
        "canada post"
    ],
    "homepage": "https:\/\/gitlab.com\/wpdesk\/wp-canada-post-shipping-method",
    "authors": [
        {
            "name": "grola",
            "email": "grola@octolize.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "psr\/log": "^1.1",
        "wpdesk\/canada-post-shipping-service": "^1.0",
        "wpdesk\/abstract-shipping": "^2.3",
        "wpdesk\/wp-woocommerce-shipping": "^3.7.9",
        "php-stubs\/woocommerce-stubs": "^6.4"
    },
    "require-dev": {
        "phpunit\/phpunit": "<7",
        "wp-coding-standards\/wpcs": "^0.14.1",
        "squizlabs\/php_codesniffer": "^3.0.2",
        "mockery\/mockery": "*",
        "10up\/wp_mock": "*",
        "phpcompatibility\/php-compatibility": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "OctolizeShippingCanadaPostVendor\\WPDesk\\WooCommerceShipping\\CanadaPost\\": "src\/WooCommerceShipping\/CanadaPost"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests\/unit"
        ]
    },
    "extra": {
        "text-domain": "wp-canada-post-shipping-method",
        "translations-folder": "lang",
        "po-files": {
            "pl_PL": "pl_PL.po",
            "en_CA": "en_CA.po"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit-unit": "phpunit --configuration phpunit-unit.xml --coverage-text --colors=never",
        "phpunit-unit-fast": "phpunit --configuration phpunit-unit.xml --no-coverage",
        "phpunit-integration": "phpunit --configuration phpunit-integration.xml --coverage-text --colors=never",
        "phpunit-integration-fast": "phpunit --configuration phpunit-integration.xml --no-coverage"
    }
}